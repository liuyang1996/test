<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liuyang.android.newquestion">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/survey"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name=".RegisterAndLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main1"/>
        <activity android:name=".Main2"/>
        <activity android:name=".RegisterAct"/>
        <activity android:name=".TestresultAct"/>
        <activity android:name=".Main3"/>
        <activity android:name=".AccountAct"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".LoginAct" />

    </application>

</manifest>